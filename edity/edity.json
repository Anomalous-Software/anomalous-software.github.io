{
  "$schema": "../../../SchemaGenerator/Schema/EdityProject.json",
  "vars": {
    "siteTitle": "Anomalous Medical"
  },
  "additionalContent": [ "Files" ],
  "compilers": [
    {
      "type": "Html",
      "extension": ".html"
    }
  ],
  "preBuildTasks": [
    {
      "name": "GetPublishRepo",
      "settings": {
        "url": "https://github.com/AnomalousMedical/anomalousmedical.github.io.git"
      }
    }
  ],
  "postBuildTasks": [
    {
      "name": "AddGithubCname",
      "settings": {
        "host": "www.anomalousmedical.com"
      }
    }
  ],
  "postPublishTasks": [
    {
      "name" : "PublishToGitRepo"
    }
  ]
}